{
  "name": "private",
  "version": "1.0.0",
  "type": "module",
  "repository": "https://github.com/Promisezhangbo/private.git",
  "author": "zhangbo <zhangbo3.vendor@sensetime.com>",
  "license": "MIT",
  "scripts": {
    "build": "pnpm --parall --filter \"./apps/{agent,main,blog}\" build",
    "dev": "pnpm --parall --filter \"./apps/{agent,main,blog}\" dev",
    "lint": "pnpm --parall --filter \"./apps/*\" lint",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@packages/prettier-config": "workspace:*",
    "@packages/eslint-config": "workspace:*",
    "@types/node": "^24.10.11",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript": "~5.9.3",
    "vite": "8.0.0-beta.13"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,vue}": [
      "eslint apps/ --ext .ts,.tsx,.js,.jsx"
    ],
    "**/*.{json,md,css,scss}": [
      "prettier --write"
    ]
  }
}
